        <Button x:Name="ButtonTwo" Content="Button" HorizontalAlignment="Center" Margin="0,314,0,0" VerticalAlignment="Top" Height="76" Width="210" Click="StartMenu_Click"/>
        <Button x:Name="ButtonThree" Content="Button" HorizontalAlignment="Center" Margin="0,442,0,0" VerticalAlignment="Top" Height="72" Width="210" Click="StartMenu_Click"/>

    </Grid>
