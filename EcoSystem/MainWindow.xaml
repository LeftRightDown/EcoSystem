<Window x:Name="MainWindowGrid" x:Class="EcoSystem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EcoSystem"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="450">
    <Grid x:Name="MainWindowGrid1" Loaded="MainWindowGrid1_Loaded">
        <DockPanel LastChildFill="False">
            <Frame x:Name="ContentFrame" Width="450" NavigationUIVisibility="Hidden"/>
        </DockPanel>
    </Grid>
</Window>
